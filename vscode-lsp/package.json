{
    "name": "scheme-lsp",
    "publisher": "OrangeBacon",
    "displayName": "scheme-lsp",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:scheme-rs"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "scheme-rs",
                "extensions": [
                    ".scm"
                ],
                "aliases": [
                    "Scheme",
                    "r5rs"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "configuration": {
            "title": "Scheme-rs Language Server",
            "properties": {
                "scheme-rs.server.path": {
                    "type": "string",
                    "description": "Path to the language server binary"
                }
            }
        },
        "configurationDefaults": {
            "[scheme-rs]": {
                "editor.insertSpaces": false,
                "editor.tabSize": 4
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.56.0",
        "@types/node": "^12.11.7",
        "typescript": "^4.1.3"
    }
}